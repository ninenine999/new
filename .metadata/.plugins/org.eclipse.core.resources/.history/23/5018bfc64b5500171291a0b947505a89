horser=1

class Animal(object):
    def __init__(self,name='动物',color='白色'):
        self.name=name
        self.color=color
class Horse(Animal):
    def __init__(self,name,color):
        super().__init__(name, color)
        horser+=1
bailongma=Horse("白龙马",'枣红色')
print(bailongma.name)
print(bailongma.color)
print(horser)

chituma=Horse('赤免马','棕色')
print(chituma.name)
print(chituma.color)
printhorser)
