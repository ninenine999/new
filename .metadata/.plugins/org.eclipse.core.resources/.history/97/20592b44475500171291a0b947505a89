class Animal(object):
    def bark(self):
        print('啊啊')
class Cat(Animal):
    def bark(self):
        print('喵')
class Dog(Animal):
    def bark(self):
        print('汪汪')
def animalBark(temp):
    temp.bark()

animalBark(maomi)
animalBark(wangcai)