def my_abs(x):
    if x>=0:
        return x
    else:
        return -x
n=my_abs(9)
print(n)



def my_abs1(x):
    if not isinstance(x, int,float):
        raise TypeError('bad operand type')
    if x>=0:
        return x
    else:
        return -x
y=my_abs1('ss')
print(y)