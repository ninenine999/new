from _operator import index
from ctypes.test.test_pickling import name

name='abcdefhncdseesss'
print(name[::])
print(name[::-1])

print(name.find('c',1,20))
print(name.rfind('c',1,20))

print(name.index('c'))
print(name.rindex('c'))
a=name.index('c')
print(name[a+1::1])